{
    "manifest_version": 3,
    "name": "Look N' Cook",
    "version": "1.0",
    "description": "Scans page for recipe and returns from in a better format",
    "host_permissions": [
        "https://mmhhfdbck6.execute-api.us-east-2.amazonaws.com/*"],
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
